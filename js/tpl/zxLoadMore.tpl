{{~it:item}}
    <li>
        <a href="{{=clientPath+item.url}}">
            <img class="newsListLeft" src="{{=item.mthumb}}"/>
            <div class="newsListRight">
                <span>{{=item.title}}</span>
                <p>{{=item.description}}</p>
            </div>
        </a>
    </li>
{{~}}